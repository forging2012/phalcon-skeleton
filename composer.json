{
    "name": "inhere/phalcon-skeleton",
    "type": "project",
    "description": "a php application skeleton of the phalcon framework",
    "keywords": ["application", "skeleton", "phalcon"],
    "homepage": "https://github.com/inhere/phalcon-skeleton",
    "license": "MIT",
    "authors": [{
        "name": "inhere",
        "email": "in.798@qq.com",
        "homepage": "http://www.yzone.net/"
    }],
    "require": {
        "php": ">=5.6",
        "fzaninotto/faker": "^1.7",
        "doctrine/annotations": "1.4.*",
        "zircote/swagger-php": "^2.0"
    },
    "require-dev": {
        "inhere/simple-print-tool": "dev-master",
        "phalcon/ide-stubs": "3.2.4",
        "phalcon/devtools": "~3.2"
    },
    "autoload": {
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    },
    "config": {
        "secure-http": false
    }
